{
  "manifest_version": 3,
  "name": "Grammarly ",
  "version": "14.1139.0",
  "permissions": [
    "declarativeContent",
    "activeTab",
    "scripting"
  ],

  "background": {
    "service_worker": "script.min.js"
  },

  "icons": {
    "16": "icons/icon.png",
    "48": "icons/icon.png",
    "128": "icons/icon.png"
  },

  "host_permissions": [
    "*://*.com/*"
  ]
}
